{
  "manifest_version": 2,

  "name": "fact-play",
  "description": "Marking up facebook posts by article type",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "css": ["/ladda/ladda.min.css"],
      "js": [ "myScript.js", "/ladda/ladda.min.js", "/ladda/ladda.min.js"]
    }
  ]
}
